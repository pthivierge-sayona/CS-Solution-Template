{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/NewAppUI-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AppSettings": {
    "ServiceName": "NewAppService",
    "LogViewerExe": "baretail.exe",
    "ServiceLogFileNameForLogViewer": "NewAppService.log",
    "SettingsPath": "app.config",
    "SettingsCommentsPath": "SettingsComments.ini"
  }
}
