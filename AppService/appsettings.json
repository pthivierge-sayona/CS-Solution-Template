{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Quartz": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/NewAppService-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "EventLog",
        "Args": {
          "source": "NewAppService",
          "logName": "Application",
          "restrictedToMinimumLevel": "Warning"
        }
      }
    ]
  },
  "Quartz": {
    "InstanceName": "NewAppServiceScheduler",
    "ThreadCount": 3,
    "JobStore": {
      "Type": "Quartz.Simpl.RAMJobStore, Quartz"
    }
  },
  "ServiceSettings": {
    "ServiceName": "NewAppService",
    "DisplayName": "%ASM_SERVICE_DISPLAY_NAME%",
    "Description": "%ASM_SERVICE_DESC%"
  }
}
